Name =
    'identifier'

SourceFile =
    Item*

Item =
    TypeDefinition

TypeDefinition =
    'type' Name '=' body:Type
    
Type = 
      Name
    | StructType
    | EnumType
    | TraitType

StructType =
    'struct' StructFieldDefinitionList

StructFieldDefinitionList =
 '{' fields:(StructField (',' StructField)* ','?)? '}'

StructField =
    Name ':' Type

EnumType =
    'enum' '{' '}'

TraitType =
    'trait' '{' '}'
